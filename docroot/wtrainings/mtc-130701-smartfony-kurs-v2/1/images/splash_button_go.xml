<params>
	<object>
		<label>Кнопка запуска модуля</label>
		<width>166</width>
		<height>31</height>
		<description>Кнопка запуска модуля</description>
		<version>2</version>
	</object>
	<page name="Параметры" w="240" h="60">
		<param type="line" x="8" y="12" w="224" h="48"/>
		<param type="label" text=" " x="11" y="6" w="6" h="13"/>
		<param type="check" id="standard" text="Использовать стандартную кнопку" def="yes" x="15" y="6" w="136" h="13">
			<events>
				<event name="check" target="lab_wait_img" action="disable" />
				<event name="check" target="lab_go_img" action="disable" />
				<event name="check" target="wait_img" action="disable" />
				<event name="check" target="go_img" action="disable" />
				<event name="uncheck" target="lab_wait_img" action="enable" />
				<event name="uncheck" target="lab_go_img" action="enable" />
				<event name="uncheck" target="wait_img" action="enable" />
				<event name="uncheck" target="go_img" action="enable" />
			</events>
		</param>
		<param type="label" id="lab_wait_img" text="Картинка до загрузки:" x="15" y="25" w="110" h="13"/>
		<param type="file" id="wait_img" text="" def="" x="105" y="24" w="100" h="13"/>
		<param type="label" id="lab_go_img" text="Картинка после загрузки:" x="15" y="41" w="110" h="13"/>
		<param type="file" id="go_img" text="" def="" x="105" y="40" w="100" h="13"/>
    </page>
</params>
